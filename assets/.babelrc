{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "14"
        }
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    "transform-es2015-destructuring",
    "transform-es2015-parameters",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    "styled-jsx/babel",
    ["import", { "libraryName": "antd" }]
  ]
}
